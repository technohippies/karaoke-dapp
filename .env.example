# This file is for scripts and utilities outside of the web app
# For web app configuration, see apps/web/.env.example

# Pinata (for IPFS uploads)
PINATA_JWT=your_pinata_jwt_here
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here

# Private key for deployment scripts
PRIVATE_KEY=your_private_key_here

# API Keys for Lit Actions (content encryption)
DEEPGRAM_API_KEY=your_deepgram_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# PKP Details (filled after minting)
PKP_TOKEN_ID=
PKP_PUBLIC_KEY=
PKP_ETH_ADDRESS=

# Lit Action CID (filled after upload)
LIT_ACTION_CID=

# Capacity Credits (filled after minting)
CAPACITY_CREDIT_NFT_ID=
CAPACITY_DELEGATION_AUTH_SIG=

# Optional: Etherscan/Basescan API for contract verification
ETHERSCAN_API_KEY=